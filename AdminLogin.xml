<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Open Cart Test Regression PW Suite" verbose="4" parallel="false" thread-count="1">
<listeners>
		<listener class-name="com.qa.listeners.ExtentReportListener"></listener>
	</listeners>
	<test name="Open Cart Home Page Test_chrome">
		 <parameter name="browser" value="chromium"></parameter>
		<classes>
			<class name="com.qa.adminTest.AdminLoginTest" />
		</classes>
	</test>
	
	<test name="Open Cart Home Page Test_firefox">
		 <parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.qa.adminTest.AdminLoginTest" />
		</classes>
	</test>
	
</suite>
